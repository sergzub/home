if [ -z "${__UTILS_SSH_INCULDE_GUARD__:-}" ]; then
readonly __UTILS_SSH_INCULDE_GUARD__=1

AssignAtVar()
{
    eval "${1#@}=\"\${2}\""
}

Repeat()
{
    local num="$1"
    local line="$2"

    while ((${#line} < num)); do
        line+="${line}"
    done

    printf '%s' "${line:0:num}"
}

fi # __UTILS_SSH_INCULDE_GUARD__
